{
  "palette": {
    "os": "#ACB0BE",
    "pink": "#F5BDE6",
    "lavender": "#B7BDF8",
    "lavender_modified": "#EED49F",
    "lavender_ahead": "#A6DA95",
    "blue": "#8AADF4",
    "wight": "#FFFFFF",
    "text": "#494D64",
    "node_green": "#7ECA9C",
    "time_green": "#A6DA95",
    "sys_info": "#A9FFB4",
    "mem_info": "#94FFA2"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:blue",
          "foreground": "p:wight",
          "powerline_symbol": "\ue0b4",
          "leading_diamond": "\u256d\u2500\ue0b6",
          "style": "diamond",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "p:blue",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "style": "diamond",
          "template": "{{ .UserName }}",
          "type": "session"
        },
        {
          "background": "p:pink",
          "foreground": "p:text",
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short",
            "max_depth": 3
          },
          "powerline_symbol": "\ue0b4",
          "style": "powerline",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "background": "p:lavender",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:lavender_modified{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:lavender_modified{{ end }}",
            "{{ if gt .Ahead 0 }}p:lavender_ahead{{ end }}"
          ],
          "foreground": "p:text",
          "style": "powerline",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true,
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "powerline_symbol": "\ue0b4",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ if gt .Working.Untracked 0 }}+{{ .Working.Untracked }} {{ end }}{{ if gt .Working.Modified 0 }}~{{ .Working.Modified }} {{ end }}{{ if gt .Working.Deleted 0 }}-{{ .Working.Deleted }} {{ end }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "p:node_green",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "style": "powerline",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "background": "p:time_green",
          "foreground": "p:text",
          "properties": {
            "time_format": "15:04"
          },
          "style": "diamond",
          "template": " \u2665 {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b4",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "p:sys_info",
          "style": "plain",
          "type": "text"
        },
        {
          "foreground": "p:sys_info",
          "properties": {
            "style": "dallas",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }}s <p:wight>\ue601</>",
          "type": "executiontime"
        },
        {
          "properties": {
            "root_icon": "\uf292 "
          },
          "style": "diamond",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "foreground": "p:mem_info",
          "style": "diamond",
          "template": " <p:wight>MEM:</> {{ round .PhysicalPercentUsed .Precision }}% ({{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB)",
          "type": "sysinfo"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:blue",
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        },
        {
          "foreground": "p:blue",
          "style": "plain",
          "template": " {{ if .Root }}#{{else}}${{end}}",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}{{ if .Root }} (Admin){{ end }}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "p:time_green",
    "foreground_templates": [
      "{{ if gt .Code 0 }}p:root_red{{ end }}"
    ],
    "template": "\u276f "
  },
  "final_space": true,
  "version": 2
}
